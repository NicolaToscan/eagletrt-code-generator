{
  "name": "eagletrt-code-generator",
  "version": "1.0.8",
  "description": "Generate dinamically code for the eagletrt telemetry",
  "main": "dist/lib/index.js",
  "bin": {
    "eagle": "dist/bin/index.js"
  },
  "scripts": {
    "transpile": "tsc --project source",
    "lint": "eslint source/**/*.ts",
    "lint:fix": "eslint --fix source/**/*.ts",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/euberdeveloper/eagletrt-code-generator.git"
  },
  "author": "Eugenio Vinicio Berretta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/euberdeveloper/eagletrt-code-generator/issues"
  },
  "keywords": ["eagletrt", "transpiler", "code-generator", "c-generator"],
  "homepage": "https://github.com/euberdeveloper/eagletrt-code-generator#readme",
  "devDependencies": {
    "@types/node": "^13.9.1",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "dree": "^2.4.12",
    "yargs": "^15.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
